<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="440.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CoordinateWidget">

        <LineChart fx:id="coordinate" prefHeight="394.0" prefWidth="450.0">
            <xAxis>
                <NumberAxis side="BOTTOM" fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" side="LEFT" />
            </yAxis>
        </LineChart>
      <Button mnemonicParsing="false" onAction="#reset" text="Reset" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="15.0" />
   <CheckBox fx:id="checkBox" mnemonicParsing="false" text="Real" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="380.0" />

</AnchorPane>
